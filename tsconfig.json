{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true,
    "outDir": "./build",
    "jsx": "react",
    "typeRoots": ["node_modules/@types", "src/types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "./node_modules",
    "./public",
    "./.cache",
    "./gatsby-config.ts",
    "./gatsby-node.ts",
    "./gatsby-browser.ts",
    "./gatsby-ssr.ts"
  ]
}
